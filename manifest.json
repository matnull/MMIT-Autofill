{
  "manifest_version": 2,
  "name": "MMIT RMA Autofill",
  "version": "1.0",
  "description": "An autofill extension for MMIT Fujitsu RMA orders",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "popup.html",
    "popup.css",
    "script.js",
    "jquery.js"
],

  "content_security_policy": "script-src 'self' 'unsafe-eval' https://code.jquery.com; object-src 'self'"
}
