{
  "manifest_version": 3,
  "name": "MMIT RMA Autofill",
  "version": "1.0",
  "description": "An autofill extension for MMIT Fujitsu RMA orders",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "popup.html"
  ]
}
